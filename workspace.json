{
  "version": 1,
  "projects": {   
    "app": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/app",
      "sourceRoot": "apps/app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:webpack-browser",
          "options": {
            "outputPath": "dist/apps/app",
            "index": "apps/app/src/index.html",
            "main": "apps/app/src/main.ts",
            "polyfills": "apps/app/src/polyfills.ts",
            "tsConfig": "apps/app/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/app/src/favicon.ico", "apps/app/src/assets"],
            "styles": ["apps/app/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/app/src/environments/environment.ts",
                  "with": "apps/app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"]
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/app/src/**/*.ts",
              "apps/app/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/app/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/app"]
        }
      }
    }, 
    "app2": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/app2",
      "sourceRoot": "apps/app2/src",
      "prefix": "app2",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:webpack-browser",
          "options": {
            "outputPath": "dist/apps/app2",
            "index": "apps/app2/src/index.html",
            "main": "apps/app2/src/main.ts",
            "polyfills": "apps/app2/src/polyfills.ts",
            "tsConfig": "apps/app2/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/app2/src/favicon.ico", "apps/app2/src/assets"],
            "styles": ["apps/app2/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/app2/src/environments/environment.ts",
                  "with": "apps/app2/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"]
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "app2:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "app2:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app2:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/app2/src/**/*.ts",
              "apps/app2/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/app2/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/app2"]
        }
      }
    }, 
    "child": {
      "projectType": "library",
      "root": "libs/child",
      "sourceRoot": "libs/child/src",
      "prefix": "ace",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "options": {
            "tsConfig": "libs/child/tsconfig.lib.json",
            "project": "libs/child/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/child/tsconfig.lib.prod.json"
            }
          }
        }
      },
      "schematics": {}
    },
    "parent": {
      "projectType": "library",
      "root": "libs/parent",
      "sourceRoot": "libs/parent/src",
      "prefix": "ace",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "options": {
            "tsConfig": "libs/parent/tsconfig.lib.json",
            "project": "libs/parent/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/parent/tsconfig.lib.prod.json"
            }
          }
        }
      },
      "schematics": {}
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "app2"
}
